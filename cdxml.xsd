  <xs:element name="elect">
    <xs:complexType name="ESD"> 
      <xs:element name="type" type="xs:string"/>
      <xs:element name="rating" type="xs:integer"/>
    </xs:complexType> 
    <xs:element name="abs_rating"> 
      <xs:element name="v_max" type="xs:integer"/>
      <xs:element name="i_max" type="xs:integer"/>
    </xs:element> 
    <xs:complexType name="op_modes"> 
      <xs:sequence>
        <xs:element name="name" type="xs:string"/>
      </xs:sequence>
    </xs:complexType> 
    <xs:complexType name="v_modes"> 
      <xs:sequence>
        <xs:element name="name" type="xs:string"/>
      </xs:sequence>
    </xs:complexType> 
    <xs:complexType name="pvt"> 
      <xs:sequence>
        <xs:element name="name" type="xs:string"/>
      </xs:sequence>
    </xs:complexType> 
  </xs:element>
</xs:schema>

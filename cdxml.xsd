  <xs:complexType/> 
  <xs:element name="date" type="xs:date"/>
  <xs:element name="manufacturer" type="xs:string"/>

  <xs:element name="mech">
    <xs:element name="smt_compatible" type="xs:string"/>
    <xs:element name="orientation_ccw" type="xs:integer"/>
    <xs:element name="mold_material" type="xs:string"/>
    <xs:element name="reflow_prof" type="xs:string"/>
    <xs:complexType name="width">
      <xs:element name="typ" type="xs:integer"/>
      <xs:element name="tol" type="xs:integer"/>
      <xs:element name="min" type="xs:integer"/>
      <xs:element name="max" type="xs:integer"/>
      <xs:element name="unit" type="xs:string"/>
    </xs:complexType> 
    <xs:complexType name="length"> 
      <xs:element name="typ" type="xs:integer"/>
      <xs:element name="tol" type="xs:integer"/>
      <xs:element name="min" type="xs:integer"/>
      <xs:element name="max" type="xs:integer"/>
      <xs:element name="unit" type="xs:string"/>
    </xs:complexType> 
    <xs:complexType name="thickness"> 
      <xs:element name="typ" type="xs:integer"/>
      <xs:element name="tol" type="xs:integer"/>
      <xs:element name="min" type="xs:integer"/>
      <xs:element name="max" type="xs:integer"/>
      <xs:element name="unit" type="xs:string"/>
    </xs:complexType> 
    <xs:complexType name="width"> 
      <xs:element name="typ" type="xs:integer"/>
      <xs:element name="tol" type="xs:integer"/>
      <xs:element name="min" type="xs:integer"/>
      <xs:element name="max" type="xs:integer"/>
      <xs:element name="unit" type="xs:string"/>
    </xs:complexType> 
    <xs:complexType name="width"> 
      <xs:element name="typ" type="xs:integer"/>
      <xs:element name="tol" type="xs:integer"/>
      <xs:element name="min" type="xs:integer"/>
      <xs:element name="max" type="xs:integer"/>
      <xs:element name="unit" type="xs:string"/>
    </xs:complexType> 
    <xs:element name="bump">
      <xs:complexType name="pitch"> 
          <xs:element name="typ" type="xs:integer"/>
          <xs:element name="tol" type="xs:integer"/>
          <xs:element name="min" type="xs:integer"/>
          <xs:element name="max" type="xs:integer"/>
          <xs:element name="unit" type="xs:string"/>
      </xs:complexType>
      <xs:complexType name="thickness"> 
          <xs:element name="typ" type="xs:integer"/>
          <xs:element name="tol" type="xs:integer"/>
          <xs:element name="min" type="xs:integer"/>
          <xs:element name="max" type="xs:integer"/>
          <xs:element name="unit" type="xs:string"/>
      </xs:complexType>
      <xs:complexType name="diameter"> 
          <xs:element name="typ" type="xs:integer"/>
          <xs:element name="tol" type="xs:integer"/>
          <xs:element name="min" type="xs:integer"/>
          <xs:element name="max" type="xs:integer"/>
          <xs:element name="unit" type="xs:string"/>
      </xs:complexType>
      <xs:complexType name="count"> 
          <xs:element name="pop" type="xs:integer"/>
          <xs:element name="unpop" type="xs:integer"/>
      </xs:complexType>
    </xs:element> 
  </xs:element> 

  <xs:complexType name="io"> 
    <xs:sequence>
      <xs:element name="pnum" type="xs:integer"/>
      <xs:element name="pname" type="xs:string"/>
      <xs:element name="sig_type" type="xs:string"/>
      <xs:element name="dir" type="xs:string"/>
      <xs:element name="mech_type" type="xs:string"/>
      <xs:element name="netlist_name" type="xs:string"/>
      <xs:element name="vdd_pin" type="xs:string"/>
      <xs:element name="gnd_pin" type="xs:string"/>
      <xs:complexType name="impedance"> 
        <xs:element name="value" type="xs:integer"/>
